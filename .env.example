# Sleeper League Configuration
# Copy this to a file named ".env" in your sleeper directory

# Your Sleeper League ID (required)
# Find this in your league URL: https://sleeper.app/leagues/YOUR_LEAGUE_ID/...
SLEEPER_LEAGUE_ID=1264686617134628864

# Optional: Update frequency settings (for future scheduling features)
UPDATE_FREQUENCY_MINUTES=30
UPDATE_DURING_GAMES_ONLY=true

# Optional: Web server configuration
WEB_SERVER_PORT=8000

# Optional: Data retention settings
KEEP_HISTORICAL_MATCHUPS=true
MAX_WEEKS_TO_KEEP=17